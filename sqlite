import sqlite3

# Подключение к базе данных и создание курсора
db = sqlite3.connect('horeo.db')
sql = db.cursor()

# Создание таблицы, если её не существует
sql.execute("""CREATE TABLE IF NOT EXISTS horeo(
    gender TEXT, 
    members TEXT,
    group_name TEXT, 
    video TEXT
)""")
db.commit()

# Ввод данных пользователя
horeo_gender = input('Пол: ')
horeo_members = input('Участники: ')
horeo_group_name = input('Название группы: ')
horeo_video = input('Ссылка на видео: ')

# Поиск записей в таблице и добавление данных или вывод сообщения
sql.execute("SELECT video FROM horeo")
result = sql.fetchall()
if not result:
    sql.execute("INSERT INTO horeo VALUES(?, ?, ?, ?)", (horeo_gender, horeo_members, horeo_group_name, horeo_video))
    db.commit()
    print("Данные добавлены")
else:
    print("Данные уже существуют")

# Закрытие соединения с базой данных
db.close()
